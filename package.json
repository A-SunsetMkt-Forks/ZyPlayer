{
  "name": "zyplayer",
  "version": "3.3.3-ext",
  "description": "simple and lightweight video client",
  "main": "./out/main/index.js",
  "author": {
    "name": "Hiram-Wong",
    "email": "admin@catni.cn"
  },
  "license": "MIT",
  "license-file": "LICENSE",
  "repository": "github:Hiram-Wong/ZyPlayer",
  "homepage": "https://github.com/Hiram-Wong/ZyPlayer",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": " electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:web": "npm run build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "^2.1.2",
    "electron-log": "^5.1.2",
    "fastify": "^4.26.2",
    "fastify-logger": "^1.0.1",
    "lodash-id": "^0.14.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@swc/core": "^1.4.8",
    "@types/cheerio": "^0.22.35",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "10.9.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "electron": "^19.1.8",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "fix-path": "^4.0.0",
    "json5": "^2.2.3",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "md-editor-v3": "^4.12.1",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "prettier": "^3.2.5",
    "qrcode.vue": "^3.4.1",
    "stylelint": "~16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "~6.0.4",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.9.0",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "url": "^0.11.3",
    "uuid-by-string": "^4.0.0",
    "v3-infinite-loading": "^1.3.1",
    "vite": "^5.2.4",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.6"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "*.{html,vue,css,sass,less}": [
      "npm run stylelint:fix"
    ]
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
